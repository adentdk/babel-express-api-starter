{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "module-resolver"
  ],
  "rules": {
    "module-resolver/use-alias": 0,
    "no-throw-literal": 0,
    "import/no-unresolved": 0
  },
  "settings": {
    "import/resolver": {
      "alias": true
    }
  }
}
