{
  "name": "babel-express-api-starter",
  "version": "1.0.0",
  "description": "Babel Express API with Sequelize & Postgre",
  "main": "index.js",
  "repository": "git@github.com:adentdk/babel-express-api-starter.git",
  "author": "Aden Trisna Daud Kurnia <adentrisnadaudkurnia@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed",
    "db:seed:all": "sequelize db:seed:all",
    "migration:generate": "sequelize migration:generate --name",
    "seed:generate": "sequelize seed:generate --name",
    "clean": "rimraf ./dist-babel",
    "dev": "nodemon â€” exec babel-node ./src/index.dev.js",
    "build": "npm run clean && run-p -l build:*",
    "build:babel": "NODE_ENV=production babel src --out-dir dist-babel --source-maps inline --copy-files",
    "preprod:babel": "yarn build:babel",
    "prod:babel": "node ./dist-babel",
    "swagger:convert:v1": "swagger-cli bundle -o ./docs/v1.json ./docs/v1.yml",
    "lint": "eslint ./src --fix"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase-admin": "^11.0.0",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^14.0.1",
    "sequelize": "^6.21.3",
    "sharp": "^0.30.7",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "^4.5.0",
    "uuid-v4": "^0.1.0",
    "winston": "^3.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-module-resolver": "^1.4.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.4.1",
    "swagger-cli": "^4.0.4"
  }
}
